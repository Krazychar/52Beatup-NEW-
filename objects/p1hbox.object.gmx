if(p1.attacking==2 || p2.grounded=false) p2.hp-=round((HBOXDAMAGE+p1.atk)/3)

if(HBOXLAUNCH=false) p1.cancel=1;
else p1.cancel=0;
}

if(time&gt;=4) instance_destroy()

if(room_speed=50 &amp;&amp; p2.hsp==0)
{
p1.hsp-=1*image_xscale
}

