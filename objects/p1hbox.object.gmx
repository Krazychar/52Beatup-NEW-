if(!place_meeting(p2.x+15*p1.image_xscale,y,ground)) p2.x+=round(HBOXDAMAGE/5)*p1.image_xscale
else p1.x-=round(HBOXDAMAGE/5)*p1.image_xscale


if(p1.rage=false) p2.hp-=HBOXDAMAGE+p1.atk

else p2.hp-=HBOXDAMAGE+p1.atk+round((p1.mhp-p1.hp)/75)

p2.hitstun=HBOXHSTUN-(round(global.p1combo*1.5))
//death effect
if(p2.hp&lt;=0)
{
instance_create(x,y,obj_winflash)
p1.launch=true
p2.y-=15
p2.OTG=true
}

if(p1.freakingslickdash=1 &amp;&amp; p1.launch=false)
{
if(p2.y&lt;700) p2.vsp=-3//!place_meeting(p2.x,p2.y+3,ground)) p2.vsp=-3
if(p1.y&lt;700) p1.vsp=-3//!place_meeting(p1.x,p1.y+4,ground)) p1.vsp=-3
}
if(HBOXLAUNCH==true) 
{
p2.vsp=-13
p2.airMovementDir=p1.image_xscale/8
p2.grounded=0
}
//Knock down (hard)
if(DOWNLAUNCH=true)
{
p2.vsp=13
p2.OTG=true
with instance_create(x,y,obj_launcheffect)
{
image_xscale=p1.image_xscale/2
image_yscale=-2
}

}
//Knock down state
if(p1.sprite_index=spr_alan_spec22)
{
p2.y-=5
p2.OTG=true
}

instance_destroy()
}

////////
if(place_meeting(x,y,p2) &amp;&amp; p2.block=true)
